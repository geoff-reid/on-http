{
    "@odata.context" : "/redfish/v1/$metadata#DCIMCoolingCollection",
    "@odata.id": "<%= url %>",
    "@odata.type": "#DCIMCoolingCollection.DCIMCoolingCollection",
    "Oem" : {},
    "Name": "DCIMCooling Collection",
    "Members@odata.count": <%= nodes.length %>,
    "Members": [
        <% nodes.forEach(function(node, i, arr) { %>
            {
                "@odata.id": "<%= basepath %>/DCIMCooling/default"
            }
            <%= ( arr.length > 0 && i < arr.length-1 ) ? ',': '' %>
        <% }); %>
    ]
}
